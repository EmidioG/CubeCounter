{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": "airbnb-base",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "no-plusplus": "off",
    "no-console": "off",
    "array-callback-return": "off",
    "consistent-return": "off",
    "max-len": "off"
  }
}
